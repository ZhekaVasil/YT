#!/bin/sh
# Wiki push

wikiFolder="YT.wiki"
wikiStatus=$(git status ./$wikiFolder --porcelain)

if [ -e ./$wikiFolder ]
then
  if [[ "$wikiStatus" = *"M $wikiFolder"* ]]
  then
  echo '${Yellow}Wiki has changed. Please commit and push the changes'
  fi
else
  echo '${Yellow}Wiki page is absent in your local repository'
fi

