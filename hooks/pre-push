#!/bin/sh
# Wiki push

wikiFolder="YT.wiki"
wikiStatus=$(git status ./$wikiFolder --porcelain)
Yellow='\033[1;33m'

if [ -e ./$wikiFolder ]
then
  if [[ "$wikiStatus" = *"M $wikiFolder"* ]]
  then
  echo -e "${Yellow}Wiki has changed. Please commit and push the changes"
  fi
else
  echo -e "${Yellow}Wiki page is absent in your local repository"
fi

